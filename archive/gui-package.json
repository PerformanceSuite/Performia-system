{
  "name": "devassist-gui",
  "version": "1.0.0",
  "description": "Web GUI for DevAssist MCP Server",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon gui-server/index.js",
    "client:dev": "cd gui-client && npm run dev",
    "build": "cd gui-client && npm run build",
    "start": "node gui-server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.16.0",
    "better-sqlite3": "^12.2.0",
    "@lancedb/lancedb": "^0.21.3",
    "@xenova/transformers": "^2.17.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
